<clickhouse>
    <users>
        <xbat>
            <profile>default</profile>
            <networks>
                <ip>::/0</ip>
            </networks>
            <password>#PASSWORD#</password>
            <access_management>0</access_management>
        </xbat>
        <xbatd>
            <profile>default</profile>
            <networks>
                <ip>::/0</ip>
            </networks>
            <password>#DAEMON_PASSWORD#</password>
            <access_management>0</access_management>

            <async_insert>1</async_insert>
            <wait_for_async_insert>1</wait_for_async_insert>
            <async_insert_max_data_size>104857600</async_insert_max_data_size>  <!-- 100MB -->
            <async_insert_busy_timeout_ms>5000</async_insert_busy_timeout_ms>
            <async_insert_use_adaptive_busy_timeout>0</async_insert_use_adaptive_busy_timeout>
            <async_insert_deduplicate>1</async_insert_deduplicate>

            <default_database>xbat</default_database>
            <databases>
                <xbat>
                </xbat>
            </databases>
        </xbatd>
    </users>
</clickhouse>