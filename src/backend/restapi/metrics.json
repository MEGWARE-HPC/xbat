    "disk": {
        "Bandwidth": {
            "metrics": {
                "disk_r_bw": "read",
                "disk_w_bw": "write"
            },
            "unit": "byte/s",
            "aggregation": "sum",
            "uri": "disk#bandwidth"
        },
        "Merged Requests": {
            "metrics": {
                "disk_rqm": "all",
                "disk_rrqm": "read",
                "disk_wrqm": "write",
                "disk_drqm": "discard"
            },
            "unit": "%",
